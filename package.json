    "@trpc/client": "^10.18.0",
    "@trpc/next": "^10.18.0",
    "@trpc/react-query": "^10.18.0",
    "@trpc/server": "^10.18.0",
    "cross-env": "^7.0.3",
    "daisyui": "^2.51.5",
    "dayjs": "^1.11.7",
    "next": "^13.2.4",
    "next-auth": "^4.21.0",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "run-p": "^0.0.0",
    "slugify": "^1.6.6",
    "superjson": "1.12.2",
    "tsx": "^3.12.7",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/eslint": "^8.21.3",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
